						"exec": [
							"let codesupportBackendUrl = pm.environment.get(\"codesupportBackend\");",
							"console.log(codesupportBackendUrl);",
							"",
							"// // Example with a full fledged SDK Request",
							"const echoPostRequest = {",
							"  url: codesupportBackendUrl + '/authenticate',",
							"  method: 'POST',",
							"  header: 'Content-Type: application/json',",
							"  body: {",
							"    mode: 'raw',",
							"    raw: JSON.stringify({ email: 'atrum@cs.dev', password: 'supersecretpassword' })",
							"  }",
							"};",
							"pm.sendRequest(echoPostRequest, function (err, res) {",
							"  if (!err) {",
							"      let result = res.json();",
							"      let token = result.response[0].token;",
							"      pm.environment.set('JWT_TOKEN', token);",
							"  } else {",
							"      console.log(err);",
							"  }",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "bearer",
					"bearer": [
						{
							"key": "token",
